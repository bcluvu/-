package practice;

public class FactorialSwitchDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(factorial(5));
		}
static int factorial(int n) {
	int result = 1;
	switch (n) {
		case 0:
		case 1:
			result = 1;
		    break;
		case 2:
		    result = 2;
		    break;
		case 3:
		    result = 6;
		    break;
		case 4:
		    result = 24;
		    break;
		case 5:
		    result = 120;
		    break;
		default:
		    result = -1;
		        }

	return result;
	}
}
